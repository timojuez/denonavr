## HIFICON SETTINGS

"hificon_key_event_handler --down --pressed"
  b:8
  
"hificon_key_event_handler --down --released"
  b:8 + Release
    
"hificon_key_event_handler --up --pressed"
  b:9

"hificon_key_event_handler --up --released"
  b:9 + Release

"echo \$volume += 5 | python3 -m hificon.hifish"
  XF86AudioRaiseVolume

"echo \$volume -= 5 | python3 -m hificon.hifish"
  XF86AudioLowerVolume

"echo \$muted = not \$muted | python3 -m hificon.hifish"
  XF86AudioMute

