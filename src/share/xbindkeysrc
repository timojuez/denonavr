## FREENON SETTINGS

"freenon_key_event_handler --down --pressed"
  b:8
  
"freenon_key_event_handler --down --released"
  b:8 + Release
    
"freenon_key_event_handler --up --pressed"
  b:9

"freenon_key_event_handler --up --released"
  b:9 + Release

"freenon_cmd MVUP"
  XF86AudioRaiseVolume

"freenon_cmd MVDOWN"
  XF86AudioLowerVolume

"freenon_cmd MUON"
  XF86AudioMute


